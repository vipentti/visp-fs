;;
;; for basic syntax highlighting
;; vim: set syntax=clojure:

;; this rule will never be matched
#nowarn "0026"

(syntax-macro matchfnexample
  [(_ sym m ...)
    (fn sym (arg)
      (match arg
        m
        ...
      ))
  ])

(matchfnexample!! name
  [(!tuple a b)
    (+ a b)]
  [_ 0])

(fn name2 (tup)
  (match tup
    [(!tuple a b) (+ a b)]
    [_ 0]))

(printfn "%A & %A"
  (name (!tuple 1 2))
  (name2 (!tuple 1 2)))
