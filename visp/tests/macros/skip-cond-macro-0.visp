;; vim: set syntax=clojure:

(syntax-macro cond_
  [(_) (failwith "unbalanced cond")]
  [(_ (test body ...))
  ]
  [(_ (test body ...) rest ...)

  ]
)

(cond_
  [(> 0 1)
    (printfn "body here")
    ()]
  [(< 1 0)
    (printfn "here")
  ]
  [_ (printfn "default")]
)
