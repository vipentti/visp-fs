;; core-macros

(syntax-macro match-fn
  [(_ (c ...) rest ...)
    (fn (arg)
      (match arg
        [c ...]
        rest ...
      ))
  ]
  [(_ sym m rest ...)
    (fn sym (arg)
      (match arg
        m
        rest ...
      ))
  ]
)
